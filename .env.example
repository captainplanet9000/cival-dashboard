# Deployment Private Key
PRIVATE_KEY=your_private_key_here

# Network RPC URLs
SONIC_TESTNET_URL=https://testnet.sonic.app/rpc
SONIC_MAINNET_URL=https://mainnet.sonic.app/rpc

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# IPFS Pinata API Keys (for metadata hosting)
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_API_KEY=your_pinata_secret_key_here

VITE_SUPABASE_URL=http://localhost:3007
VITE_SUPABASE_ANON_KEY=your_anonymous_key_here

# API Configuration
NEXT_PUBLIC_API_URL=https://api.your-service.com/v1
NEXT_PUBLIC_ELIZAOS_API_URL=https://elizaos-api.your-service.com/v1

# Database Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Authentication
NEXT_PUBLIC_AUTH_API_URL=https://auth.your-service.com
NEXT_PUBLIC_AUTH_CLIENT_ID=your-auth-client-id
AUTH_SECRET=your-auth-secret-key

# Real-time Configuration
NEXT_PUBLIC_WEBSOCKET_URL=wss://realtime.your-service.com

# Feature Flags
NEXT_PUBLIC_ENABLE_REAL_TIME=true
NEXT_PUBLIC_ENABLE_ADVANCED_METRICS=true

# Logging
LOG_LEVEL=info 