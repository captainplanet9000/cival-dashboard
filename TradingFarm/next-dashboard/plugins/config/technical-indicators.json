{
  "plugin": "technical-indicators",
  "version": "1.1.2",
  "settings": {
    "indicators": [
      "rsi", "macd", "bollinger", "ema", "sma", "fibonacci", "ichimoku"
    ],
    "timeframes": [
      "1m", "5m", "15m", "1h", "4h", "1d"
    ],
    "defaultPeriods": {
      "rsi": 14,
      "macd": {
        "fast": 12,
        "slow": 26,
        "signal": 9
      },
      "bollinger": {
        "period": 20,
        "stdDev": 2
      },
      "ema": [9, 21, 50, 200],
      "sma": [10, 20, 50, 200]
    },
    "calculateOnDemand": true,
    "cacheResults": true,
    "cacheTTL": 300000
  }
}
