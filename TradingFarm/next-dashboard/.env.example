# Trading Farm Dashboard Environment Configuration (EXAMPLE)
# Copy this file to .env.local and fill in the real values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-url.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Development Flags
NEXT_PUBLIC_USE_MOCK_DATA=false
NEXT_PUBLIC_MOCK_API_ENABLED=false
NEXT_PUBLIC_FORCE_MOCK_MODE=false

# Application Settings
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Trading Farm Dashboard

# ElizaOS Configuration
NEXT_PUBLIC_ELIZAOS_API_URL=https://elizaos-api.tradingfarm.io/api
NEXT_PUBLIC_ELIZAOS_AGENT_PREFIX=eliza_trading_agent_

# Trading Farm Backend
NEXT_PUBLIC_TRADING_FARM_API_URL=https://api.tradingfarm.io/api

# Memory Systems (Contact admin for API keys)
NEXT_PUBLIC_COGNEE_API_KEY=your-cognee-api-key
NEXT_PUBLIC_GRAPHITI_API_KEY=your-graphiti-api-key

# MCP Services
NEXT_PUBLIC_MCP_NEON_ENDPOINT=https://neon-mcp.tradingfarm.io
NEXT_PUBLIC_MCP_NEON_API_KEY=your-neon-api-key
NEXT_PUBLIC_MCP_BROWSERBASE_ENDPOINT=https://browserbase-mcp.tradingfarm.io
NEXT_PUBLIC_MCP_HYPERLIQUID_ENDPOINT=https://hyperliquid-mcp.tradingfarm.io

# GraphQL Endpoint
NEXT_PUBLIC_GRAPHQL_API_URL=https://graphql.tradingfarm.io/v1/graphql
NEXT_PUBLIC_GRAPHQL_WS_URL=wss://graphql.tradingfarm.io/v1/graphql

# Dashboard Settings
NEXT_PUBLIC_AUTO_REFRESH_INTERVAL=30000
NEXT_PUBLIC_DEFAULT_CURRENCY=USD 