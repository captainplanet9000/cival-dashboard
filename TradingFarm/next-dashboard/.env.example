# Next.js Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_SOCKET_URL=http://localhost:3001

# Socket Server Configuration
SOCKET_SERVER_PORT=3001
SOCKET_SERVER_CORS_ORIGIN=http://localhost:3000
SOCKET_SERVER_LOG_LEVEL=info

# ElizaOS AI Service
NEXT_PUBLIC_ELIZAOS_ENABLED=true
NEXT_PUBLIC_GEMMA_API_KEY=your-gemma-api-key
GEMMA_API_BASE_URL=https://generativelanguage.googleapis.com/v1beta

# Market Data Service
NEXT_PUBLIC_COINGECKO_API_KEY=your-coingecko-api-key
MARKET_DATA_REFRESH_INTERVAL=30000

# Web3 Configuration
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your-walletconnect-project-id
NEXT_PUBLIC_ENABLE_TESTNET=false

# Blockchain RPC URLs
NEXT_PUBLIC_ETHEREUM_RPC_URL=https://eth-mainnet.alchemyapi.io/v2/your-api-key
NEXT_PUBLIC_POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/your-api-key
NEXT_PUBLIC_ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/your-api-key
NEXT_PUBLIC_OPTIMISM_RPC_URL=https://opt-mainnet.g.alchemy.com/v2/your-api-key
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Knowledge Base Integration
NEXT_PUBLIC_KNOWLEDGE_BASE_ENABLED=true
NEXT_PUBLIC_PINECONE_API_KEY=your-pinecone-api-key
NEXT_PUBLIC_PINECONE_ENVIRONMENT=your-pinecone-environment
NEXT_PUBLIC_PINECONE_INDEX=trading-farm-knowledge
