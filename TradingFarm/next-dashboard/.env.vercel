# Vercel Environment Variables for Trading Farm Dashboard
# Copy these variables to your Vercel project configuration

# Supabase Configuration (Public variables)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Supabase Service Role (Server-side only, not exposed to client)
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Backend Service URLs
NEXT_PUBLIC_BACKEND_API_URL=https://your-trading-service.up.railway.app
NEXT_PUBLIC_WEBSOCKET_URL=wss://your-trading-service.up.railway.app

# Feature Flags
NEXT_PUBLIC_ENABLE_LIVE_TRADING=false
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true
NEXT_PUBLIC_ENABLE_AGENT_ORCHESTRATION=false

# Analytics and Monitoring
NEXT_PUBLIC_ANALYTICS_ENABLED=true

# Encryption (for client-side encryption where needed)
NEXT_PUBLIC_ENCRYPTION_IV=your-public-iv-here

# Rate Limiting
NEXT_PUBLIC_API_RATE_LIMIT=100

# Deployment Environment
NEXT_PUBLIC_ENVIRONMENT=production

# Application Version
NEXT_PUBLIC_APP_VERSION=1.0.0
